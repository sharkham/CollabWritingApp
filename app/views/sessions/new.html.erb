<h1>Log In</h1>
<div class="error_explanation">
<%= flash[:message] if flash[:message] %>
</div>
<%= form_for :user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Log In" %>


<% end %>
