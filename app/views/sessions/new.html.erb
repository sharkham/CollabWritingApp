<h1>Log In</h1>

<div class="error_explanation">
<%= flash[:incorrect_login] if flash[:incorrect_login] %>
</div>

<%= form_for :user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Log In" %>


<% end %>
